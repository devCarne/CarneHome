<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stu.kms.carnehome.mapper.AttachMapper">

    <insert id="insert">
        insert into CARNEHOME_ATTACH values (#{uuid}, #{uploadPath}, #{fileName}, #{fileType}, #{postNo})
    </insert>

    <delete id="deleteAll">
        delete from CARNEHOME_ATTACH where POSTNO = #{postNo}
    </delete>

    <select id="getAttachList" resultType="stu.kms.carnehome.domain.PostAttachVO">
        select * from CARNEHOME_ATTACH where POSTNO = #{postNo}
    </select>
</mapper>